{
  "name": "azam",
  "version": "1.0.0",
  "description": "Azam-Integration",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "clean": "lb-clean dist *.tsbuildinfo",
    "lint": "npm run prettier:check && npm run eslint",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "npm run eslint -- --fix",
    "pretest": "npm run clean && npm run build",
    "test": "lb-mocha --allow-console-logs \"dist/__tests__\"",
    "posttest": "npm run lint",
    "test:dev": "lb-mocha --allow-console-logs dist/__tests__/**/*.js && npm run posttest",
    "docker:build": "docker build -t azam .",
    "docker:run": "docker run -p 3000:3000 -d azam",
    "deploy:test:airtel-ussd": "NODE_ENV=testing PORT=8080 pm2 start ./index.js -i 1 --name airtel-ussd:8080",
    "deploy:test:airtel-ussd-push": "NODE_ENV=testing PORT=4301 pm2 start ./index.js -i 1 --name airtel-ussd-push:4301",
    "deploy:test:halotel-ussd": "NODE_ENV=testing PORT=8080 pm2 start ./index.js -i 1 --name halotel-ussd:8080",
    "deploy:test:halotel-ussd-push": "NODE_ENV=testing PORT=8080 pm2 start ./index.js -i 1 --name halotel-ussd-push:8080",
    "deploy:test:tigo-ussd": "NODE_ENV=testing PORT=8080 pm2 start ./index.js -i 1 --name tigo-ussd:8080",
    "deploy:test:tigo-ussd-push": "NODE_ENV=testing PORT=8080 pm2 start ./index.js -i 1 --name tigo-ussd-push:8080",
    "deploy:test:vodacom-ussd": "NODE_ENV=testing PORT=8080 pm2 start ./index.js -i 1 --name vodacom-ussd:8080",
    "deploy:test:vodacom-ussd-push": "NODE_ENV=testing PORT=8080 pm2 start ./index.js -i 1 --name vodacom-ussd-push:8080",
    "deploy:test:zantel-ussd": "NODE_ENV=testing PORT=8080 pm2 start ./index.js -i 1 --name zantel-ussd:8080",
    "deploy:test:zantel-ussd-push": "NODE_ENV=testing PORT=8080 pm2 start ./index.js -i 1 --name zantel-ussd-push:8080",
    "deploy:prod:airtel-ussd": "NODE_ENV=production PORT=4300 pm2 start ./index.js -i 1 --name airtel-ussd:4300",
    "deploy:prod:airtel-ussd-push": "NODE_ENV=production PORT=4301 pm2 start ./index.js -i 1 --name airtel-ussd-push:4301",
    "deploy:prod:halotel-ussd": "NODE_ENV=production PORT=8080 pm2 start ./index.js -i 1 --name halotel-ussd:8080",
    "deploy:prod:halotel-ussd-push": "NODE_ENV=production PORT=8080 pm2 start ./index.js -i 1 --name halotel-ussd-push:8080",
    "deploy:prod:tigo-ussd": "NODE_ENV=production PORT=8080 pm2 start ./index.js -i 1 --name tigo-ussd:8080",
    "deploy:prod:tigo-ussd-push": "NODE_ENV=production PORT=8080 pm2 start ./index.js -i 1 --name tigo-ussd-push:8080",
    "deploy:prod:vodacom-ussd": "NODE_ENV=production PORT=8080 pm2 start ./index.js -i 1 --name vodacom-ussd:8080",
    "deploy:prod:vodacom-ussd-push": "NODE_ENV=production PORT=8080 pm2 start ./index.js -i 1 --name vodacom-ussd-push:8080",
    "deploy:prod:zantel-ussd": "NODE_ENV=production PORT=8080 pm2 start ./index.js -i 1 --name zantel-ussd:8080",
    "deploy:prod:zantel-ussd-push": "NODE_ENV=production PORT=8080 pm2 start ./index.js -i 1 --name zantel-ussd-push:8080",
    "migrate": "node ./dist/migrate",
    "prestart": "npm run build",
    "start": "node -r source-map-support/register .",
    "prepublishOnly": "npm run test"
  },
  "repository": {
    "type": "git"
  },
  "author": "",
  "license": "",
  "files": [
    "README.md",
    "index.js",
    "index.d.ts",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@loopback/authentication": "^4.2.9",
    "@loopback/authentication-passport": "^2.1.9",
    "@loopback/boot": "^2.3.5",
    "@loopback/context": "^3.9.2",
    "@loopback/core": "^2.9.1",
    "@loopback/openapi-v3": "^3.4.5",
    "@loopback/repository": "^2.9.0",
    "@loopback/rest": "^5.2.0",
    "@loopback/rest-explorer": "^2.2.6",
    "@loopback/service-proxy": "^2.3.4",
    "@types/amqplib": "^0.5.13",
    "@types/async": "^3.2.3",
    "@types/bcrypt": "^3.0.0",
    "@types/crypto-js": "^3.1.47",
    "@types/nodemailer": "^6.4.0",
    "@types/sha256": "^0.2.0",
    "amqplib": "^0.5.6",
    "async": "^3.2.0",
    "bcrypt": "^4.0.1",
    "crypto-js": "^4.0.0",
    "jsonwebtoken": "^8.5.1",
    "loopback-connector-mysql": "^5.4.3",
    "md5": "^2.2.1",
    "moment": "^2.26.0",
    "nodemailer": "^6.4.10",
    "npm": "^6.14.6",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "pm2": "^4.4.0",
    "request": "^2.88.2",
    "request-promise": "^4.2.5",
    "sha256": "^0.2.0",
    "tslib": "^2.0.0",
    "winston": "^3.2.1",
    "xml2json": "^0.12.0"
  },
  "devDependencies": {
    "@loopback/build": "^6.1.0",
    "@loopback/eslint-config": "^8.0.3",
    "@loopback/testlab": "^3.2.0",
    "@types/md5": "^2.2.0",
    "@types/node": "^10.17.26",
    "@types/passport": "^1.0.3",
    "@types/passport-jwt": "^3.0.3",
    "@types/request-promise": "^4.1.46",
    "@types/xml2json": "^0.11.0",
    "@typescript-eslint/eslint-plugin": "^2.31.0",
    "@typescript-eslint/parser": "^2.31.0",
    "eslint": "^7.3.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-eslint-plugin": "^2.2.1",
    "eslint-plugin-mocha": "^7.0.0",
    "source-map-support": "^0.5.19",
    "typescript": "~3.9.5"
  }
}
